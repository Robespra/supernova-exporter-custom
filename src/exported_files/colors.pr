import 'package:flutter/material.dart';

class AppColors {
  {[ const brand = ds.currentBrand() /]}
  {[ const tokens = ds.tokensByType("Color", brand.id) /]}
  {[ for token in tokens ]}
    {[ if token.description ]}
  {{ createDocumentationComment(token.description, "  ") }}
    {[/]}
  static const Color {{ token.name }} = {[ inject "export_color_value" context token.value /]};
  {[/]}
  AppColors._();
}
